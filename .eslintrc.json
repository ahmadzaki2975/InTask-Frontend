{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "warn",
    "indent" : [
      "warn",
      2
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "no-unused-vars": [
      "error"
    ]
  }
}
